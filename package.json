{
  "name": "brickbuddy-api-docs",
  "version": "1.0.0",
  "description": "Documentation and testing portal for the BrickBuddy API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "server": "nodemon src/server.js",
    "client": "PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "api",
    "documentation",
    "brickbuddy",
    "interactive",
    "reference"
  ],
  "author": "BrickBuddy Team",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5003"
}
